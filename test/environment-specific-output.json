{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.ApiManagement/service/apis",
      "name": "apim1/example-api-development",
      "properties": {
        "displayName": "Example-Api - Development",
        "apiRevision": "1",
        "subscriptionRequired": true,
        "path": "example-api",
        "protocols": ["https"],
        "isCurrent": true
      },
      "resources": [
        {
          "type": "policies",
          "name": "policy",
          "dependsOn": ["example-api-development"],
          "properties": {
            "value": "<policies><inbound><base />\n<set-backend-service base-url=\"https://development.example.com\" />\n</inbound><backend><base /></backend><outbound><base /></outbound><on-error><base /></on-error></policies>",
            "format": "xml"
          },
          "apiVersion": "2019-01-01"
        }
      ],
      "apiVersion": "2019-01-01"
    },
    {
      "type": "Microsoft.ApiManagement/service/apis/operations",
      "name": "apim1/example-api-development/get-data",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/apis', 'apim1', 'example-api-development')]"
      ],
      "properties": {
        "displayName": "Get-Data",
        "method": "GET",
        "urlTemplate": "/data",
        "templateParameters": [],
        "responses": []
      },
      "resources": [],
      "apiVersion": "2019-01-01"
    }
  ]
}
